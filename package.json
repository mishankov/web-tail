{
  "name": "web-tail",
  "version": "0.6.0",
  "private": true,
  "bin": "dist/index.js",
  "scripts": {
    "build:js": "rollup -c",
    "build:exec": "pkg . --out-path dist",
    "build:docker": "docker build . -t mishankov/web-tail",
    "build": "npm run build:js && npm run build:exec && npm run build:docker",
    "start": "node dist/index.js",
    "start:docker": "docker run -p 4444:4444 -d mishankov/web-tail",
    "dev": "rollup -c -w",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "gen": "node utils/generate_logs.js"
  },
  "pkg": {
    "assets": [
      "dist/public/**/*"
    ],
    "output": "dist"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-run": "^3.1.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/ssh2": "^0.5.51",
    "@types/ws": "^8.2.2",
    "pkg": "^5.8.1",
    "prettier": "3.3.3",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.2.2",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^5.1.13",
    "svelte-check": "^4.0.6",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "express": "^4.19.2",
    "express-rate-limit": "^6.2.0",
    "open": "^8.4.0",
    "ssh2": "^1.5.0",
    "tail": "^2.2.4",
    "toml": "^3.0.0",
    "ws": "^8.17.1"
  }
}
